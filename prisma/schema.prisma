datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model todo {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  title     String
  isDone    Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}
